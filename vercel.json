{
  "rewrites": [
    {
      "source": "/api/script/get_script.php",
      "destination": "/api/script/get_script.php.js"
    },
    {
      "source": "/minedrop/console",
      "destination": "/console.html"
    },
    {
      "source": "/minedrop/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/minedrop",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}

